{
  "name": "vsp-nxb-website",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env.clientDev next dev",
    "build": "next build",
    "buildDev": "cp -f .env.clientDev .env && next build",
    "buildProd": "cp -f .env.clientProd .env && next build",
    "start": "env-cmd -f .env.clientProd next start",
    "lint": "next lint",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "verify-commit": "verify-commit-msg",
    "postinstall": "git-scm-hooks",
    "release": "bumpp -r",
    "gen-icon": "csvg-to-font -c t4font",
    "standalone": "node server.js"
  },
  "dependencies": {
    "ahooks": "^3.7.8",
    "autoprefixer": "^10.4.16",
    "brainless-token-manager": "^1.3.3",
    "classnames": "^2.3.2",
    "cookies-next": "^4.0.0",
    "dayjs": "^1.11.10",
    "jotai": "^2.4.2",
    "next": "^13.5.3",
    "next-i18next": "^11.3.0",
    "patch-package": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.30.7",
    "umi-request": "^1.4.0"
  },
  "devDependencies": {
    "@hunghg255/eslint-config-react": "^0.0.18",
    "@next/bundle-analyzer": "^12.3.4",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/node": "18.7.16",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "bumpp": "^9.2.0",
    "cross-env": "^7.0.3",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "csvg-to-font": "^1.0.8",
    "env-cmd": "^10.1.0",
    "eslint": "^8.50.0",
    "eslint-config-next": "^13.5.3",
    "git-scm-hooks": "^0.0.6",
    "postcss": "^8.4.30",
    "prettier": "^2.8.8",
    "sass": "^1.68.0",
    "tailwindcss": "^3.3.3",
    "typescript": "4.8.3",
    "verify-commit-msg": "^0.0.11"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "git-hooks": {
    "pre-commit": "npm run lint",
    "commit-msg": "npm run verify-commit"
  }
}
